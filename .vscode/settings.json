{
  // Focusing flutter so ignore server when searching
  "search.exclude": {
    "**/server": true
  },
  "files.exclude": {
    // #### Root
    ".netlify": true,
    "tsconfig-base.json": true,
    "tsconfig-cjs.json": true,
    "**/dist": true,
    "**/.git": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/error.vue": true,
    "**/options.vite": true,
    "**/node_modules": true,
    "**/firebase.json": true,
    "**/formkit.config.js": true,
    "**/package-lock.json": true,
    "**/prettier.config.cjs": true,

    // #### flutter
    "**/.dart_tool": true,
    "**/mobile.iml": true,
    "**/analysis_options.yaml": true,
    "flutter/pubspec.lock": true,
    "flutter/build": true,
    "**/macos": true,
    "**/web": true,
    "**/.flutter-plugins": true,
    "**/.metadata": true,
    "**/*.freezed.dart": true,
    "**/*.g.dart": true,
    "**/.flutter-plugins-dependencies": true,

    // Temp while focusiong on clean archi/di
    "**/android": true,
    "**/ios": true,
    "**/l10n": true,

    // #### Focus Flutter: start

    "assets": true,
    "components": true,
    "composables": true,
    "content": true,
    "layouts": true,
    "pages": true,
    "plugins": true,
    "public": true,
    "utils": true,
    "tailwind.config.js": true,
    "package.json": true,
    "app.vue": true,
    "tsconfig.json": true,
    ".env": true,
    "netlify": true,

    // #### Focus Flutter: end
    // #### Focus Nuxt: start

    // "flutter": true,

    // #### Focus Nuxt: end

    // Netlify functions
    // "netlify/functions/esm-muxter/Turboship/nuxt": true,
    // "netlify/functions/esm-muxter/Turboship/flutter": true,

    // ##### Leave forever because we always want to ignore and don't wanna mess up due to missing comma
    "**/bun.lockb": true
  },
  "editor.codeActionsOnSave": {
    "quickfix.insertSemicolon": true
  },
  "dart.analysisExcludedFolders": [
    "netlify/functions/esm-muxter/Turboship/flutter",
    "netlify/functions/esm-muxter/TurboshipCJS/flutter"
  ],
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#000",
    "titleBar.activeBackground": "#41b883",
    "titleBar.inactiveBackground": "#3b8070",
    "titleBar.inactiveForeground": "#000000CC"
  },
  "cSpell.words": ["headlessui", "Pinia", "Reactionable", "realestate", "toastify", "usecases", "vuedraggable"]
}
