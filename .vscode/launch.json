{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "client: chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "cwd": "${fileWorkspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server: nuxt",
      "outputCapture": "std",
      "cwd": "${fileWorkspaceFolder}",
      "program": "${fileWorkspaceFolder}/node_modules/nuxi/bin/nuxi.mjs",
      "args": ["dev"]
    },
    {
      "name": "iPhone 15 Pro Max dev(debug)",
      "type": "dart",
      "request": "launch",
      "program": "flutter/lib/main_development.dart",
      "flutterMode": "debug",
      "deviceId": "3CAE8793-2B07-4522-917B-C56BC2E84F9D",
      "args": ["--flavor", "development", "--dart-define=FLAVOR=development"]
    },
    {
      "type": "dart",
      "request": "launch",
      "flutterMode": "debug",
      "program": "lib/main_development.dart",
      "name": "Loi iPhone(device)",
      "deviceId": "00008110-000218C20C0B801E"
    },
    {
      "type": "dart",
      "request": "launch",
      "flutterMode": "debug",
      "name": "Flutter for web",
      "program": "flutter/lib/main.dart",
      "args": ["-d", "chrome", "--flavor", "development", "--dart-define=FLAVOR=development"]
    }
  ],
  "compounds": [
    {
      "name": "nuxi server",
      "configurations": ["server: nuxt", "client: chrome"]
    }
  ]
}
